<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A6C582"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/text_home8"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you"
        android:textAlignment="center"
        android:textSize="16sp"
        android:text="«Барт мозг болит»."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.927" />

    <TextView
        android:id="@+id/text_home9"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you2"
        android:textAlignment="center"
        android:textSize="16sp"
        android:text="«Что, людь мне видит? Я записался в армию короля Радозвида. И у меня приказ — защищать лодки»."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.927" />

    <TextView
        android:id="@+id/text_home6"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you2"
        android:textAlignment="center"
        android:textSize="16sp"
        android:text="«Говорят, что Голиаф когда-то был славным рыцарем. К сожалению, однажды он прогневал Владычицу Озера, и та превратила его в чудовище»."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.629" />

    <TextView
        android:id="@+id/text_home7"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you"
        android:textAlignment="center"
        android:textSize="15sp"
        android:text="«Супдетруп, старый тролльский рецепт. Там, внутри, только хорошее: улитки, жабы, черепа... всякая такая зелень, чтоб пахла... А под конец – один накер для вкуса»."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.629" />

    <TextView
        android:id="@+id/text_home4"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you"
        android:textAlignment="center"
        android:textSize="16sp"
        android:text="С нами лучше не балуй лишь бы цел остался..."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.327" />

    <TextView
        android:id="@+id/text_home5"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you2"
        android:textAlignment="center"
        android:textSize="16sp"
        android:text="«Большой. Злой. Мерзкий. В каналах сидит»."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.327" />

    <TextView
        android:id="@+id/text_home10"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you2"
        android:textAlignment="center"
        android:textSize="16sp"
        android:text="«Жить и не слышать божественного сочетания звуков „ниточка карачунницы” — все равно что лизать улиток через сукно»."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027" />

    <TextView
        android:id="@+id/text_home3"
        android:layout_width="170dp"
        android:layout_height="154dp"
        android:background="@drawable/material_you"
        android:textAlignment="center"
        android:textSize="16sp"
        android:text="«Ходят гули вдоль дороги, съели руки, съели ноги»."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027" />

</androidx.constraintlayout.widget.ConstraintLayout>